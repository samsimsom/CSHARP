@page "/"

<h1>Hello, world!</h1>
<p>Welcome to your new app.</p>

@for (int i = 0; i < 10; i++)
{
    @i;
    if (i == 9)
    {
        <br/>
        Test();
        <p>@Number</p>
    }

}

<SurveyPrompt Title="How is Blazor working for you?"/>

@code
{
    public int Number { get; set; }
    
    public void Test()
    {
        for (int i = 0; i < 10; i++)
        {
            Number = i;
        }
    }
}
